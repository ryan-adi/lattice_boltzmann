        <dt type="float">1.0</dt>
        <relaxationTime type="float">0.55</relaxationTime>
    </Fluid>

    <InitialConditions>
        <microVelDir type="np.array" number="int">1 2</microVelDir>
        <microVelVal type="np.array" number="float">3.0 1.0</microVelVal>
        <velocity type="np.array" number="float">1.0 0.0</velocity>
