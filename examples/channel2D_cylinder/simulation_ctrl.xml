<data caseName="channel2D_cylinder">
    <SimSetting>
        <fps type="int">10</fps>
        <export type="int">10</export>
        <dt type="float">1.0</dt>
        <endTime type="float">2000.0</endTime>
        <removePng type="int">1</removePng>
    </SimSetting>

    <Geometry>
        <lx type="float">1.0</lx>
        <ly type="float">1.0</ly>
        <nx type="int">200</nx>
        <ny type="int">50</ny>
    </Geometry>

    <!--
    <Obstacle>
        <shape type="str">box</shape>
        <bb_min type="np.array" number="int">18 22</bb_min>
        <bb_max type="np.array" number="int">22 28</bb_max>
    </Obstacle>
    -->

    <Obstacle>
        <shape type="str">circle</shape>
        <radius type="int">5</radius>
        <center type="np.array" number="int">20 25</center>
    </Obstacle>


    <Fluid>
        <viscosity type="float">0.002</viscosity>
